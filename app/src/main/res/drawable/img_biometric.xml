<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <bitmap android:src="@drawable/img_ring" />
    </item>

    <item
        android:bottom="@dimen/_20sdp"
        android:end="@dimen/_20sdp"
        android:start="@dimen/_20sdp"
        android:top="@dimen/_20sdp">
        <shape
            android:shape="oval">
            <solid
                android:color="@color/green" />
        </shape>
    </item>

    <item
        android:width="@dimen/_90sdp"
        android:height="@dimen/_90sdp"
        android:gravity="center">
        <bitmap
            android:src="@drawable/img_finger_print" />
    </item>

</layer-list>